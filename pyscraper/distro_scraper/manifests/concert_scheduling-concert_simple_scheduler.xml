<package>
  <name>concert_simple_scheduler</name>
  <version>0.7.0</version>
  <description>
    This ROS package provides a simple, experimental, resource
    scheduler for the Robotics in Concert project.
  </description>
  <maintainer email="jack.oquin@gmail.com">Jack O'Quin</maintainer>
  <license>BSD</license>  

  <url type="website">http://wiki.ros.org/concert_simple_scheduler</url>
  <url type="repository">https://github.com/utexas-bwi/rocon_experimental.git</url>
  <url type="bugtracker">https://github.com/utexas-bwi/rocon_experimental/issues</url>
  <author>Jack O'Quin</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>concert_msgs</build_depend>
  <build_depend>concert_scheduler_requests</build_depend>
  <build_depend>roslint</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>scheduler_msgs</build_depend>
  <build_depend>unique_id</build_depend>

  <run_depend>concert_msgs</run_depend>
  <run_depend>concert_scheduler_requests</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>scheduler_msgs</run_depend>
  <run_depend>unique_id</run_depend>

  
  <run_depend>python-yaml</run_depend>
  <run_depend>rocon_app_manager_msgs</run_depend>
  <run_depend>rocon_std_msgs</run_depend>
  <build_depend>rostest</build_depend>
  <test_depend>rosunit</test_depend>

  
  <build_depend>python-catkin-pkg</build_depend>

  <export>
    <rosdoc config="rosdoc.yaml" />
  </export>

</package>