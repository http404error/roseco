<?xml version="1.0" ?><package>
  <name>concert_resource_pool</name>
  <version>0.7.0</version>
  <description>
    Python interfaces for managing a pool of ROCON scheduler resources.
  </description>
  <maintainer email="jack.oquin@gmail.com">Jack O'Quin</maintainer>
  <license>BSD</license>  

  <url type="website">http://wiki.ros.org/concert_resource_pool</url>
  <url type="repository">
    https://github.com/utexas-bwi/concert_scheduling.git
  </url>
  <url type="bugtracker">
    https://github.com/utexas-bwi/concert_scheduling/issues
  </url>
  <author>Jack O'Quin</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>concert_msgs</build_depend>
  <build_depend>rocon_app_manager_msgs</build_depend>
  <build_depend>roslint</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>scheduler_msgs</build_depend>
  <build_depend>unique_id</build_depend>

  <run_depend>concert_msgs</run_depend>
  <run_depend>rocon_app_manager_msgs</run_depend>
  <run_depend>rocon_std_msgs</run_depend>
  <run_depend>rocon_uri</run_depend>
  <run_depend>rospy</run_depend>
  <run_depend>scheduler_msgs</run_depend>
  <run_depend>unique_id</run_depend>

  <!-- these dependencies are only for testing -->
  <build_depend>rostest</build_depend>
  <run_depend>python-yaml</run_depend>
  <test_depend>rosunit</test_depend>

  <!-- documentation dependencies -->
  <build_depend>python-catkin-pkg</build_depend>

  <export>
    <rosdoc config="rosdoc.yaml"/>
  </export>

</package>