<?xml version="1.0" ?><package>
  <name>rosh_desktop</name>
  <version>1.0.2</version>
  <description>ROSH meta-plugin for the ROS 'desktop' variant.</description>
  <maintainer email="dan@lazewatsky.com">Dan Lazewatsky</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/rosh_desktop</url>
  <url type="bugtracker">https://github.com/OSUrobotics/rosh_desktop_plugins/issues</url>

  <author>Ken Conley</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>rosh</run_depend>
  <run_depend>rosh_robot</run_depend>
  <run_depend>rosh_visualization</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <!-- <test_depend>rosh</test_depend> -->
  <!-- <test_depend>rosh_robot</test_depend> -->
  <!-- <test_depend>rosh_visualization</test_depend> -->

</package>