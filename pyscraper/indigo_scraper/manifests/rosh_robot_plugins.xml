<?xml version="1.0" ?><package>
  <name>rosh_robot_plugins</name>
  <version>1.0.1</version>
  <description>ROSH related packages. This is a temporary stack that is expected to go away after the Diamondback release. For C Turtle and Diamondback it provides a convenient way to install rosh until it is properly stabilized.</description>
  <maintainer email="dan@lazewatsky.com">Dan Lazewatsky</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/rosh_robot_plugins</url>
  <url type="bugtracker">https://github.com/OSUrobotics/rosh_robot_plugins/issues</url>

  <author>Ken Conley</author>

  <!-- Dependencies which this package needs to build itself. -->
  <buildtool_depend>catkin</buildtool_depend>

  <!-- Dependencies needed to compile this package. -->


  <!-- Dependencies needed after this package is compiled. -->
  <run_depend>rosh_geometry</run_depend>
  <run_depend>rosh_common</run_depend>
  <run_depend>rosh_robot</run_depend>

  <!-- Dependencies needed only for running tests. -->
  <export>
    <metapackage/>
  </export>
</package>